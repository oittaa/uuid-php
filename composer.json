    "keywords": [
        "uuid",
        "identifier",
        "guid"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "UUID\\": "src/"
        }
    }
}
